pipeline{
    agent any
    stages{
        stage("compilePhase"){
            steps{
                bat """
                	cd firstRestService
                	mvn compile
        		"""
                }
            }

        stage("testPhase"){
            steps{
                bat "cd firstRestService"
                bat "mvn test"
                }
        }

        stage("packagePhase"){
        steps{
            bat "cd firstRestService"
            bat "mvn clean package"
                }
            }
        }
}
